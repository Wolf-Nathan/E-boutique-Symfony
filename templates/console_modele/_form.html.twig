{{ form_start(form) }}
    {#{{ form_widget(form) }}#}
    {{ form_errors(form) }}
    <div class="form-group">
        {{ form_label(form.nom, "Modèle", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        <div class="col-sm-10">
            {{ form_errors(form.nom) }}
            {{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
        </div>
    </div>

    <div class="form-group">
        {{ form_label(form.console, "Console", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        <div class="col-sm-10">
            {{ form_errors(form.console) }}
            {{ form_widget(form.console, {'attr': {'class': 'form-control'}}) }}
        </div>
    </div>

    <div class="form-group">
        {{ form_label(form.prix, "Prix", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        <div class="col-sm-10">
            {{ form_errors(form.prix) }}
            {{ form_widget(form.prix, {'attr': {'class': 'form-control'}}) }}
        </div>
    </div>

    <div class="form-group">
        {{ form_label(form.dateSortie, "Date de sortie") }}
        <div class="col-sm-10">
            {{ form_errors(form.dateSortie) }}
            {{ form_widget(form.dateSortie) }}
        </div>
    </div>
    {{ form_rest(form) }}
    <button class="btn">{{ button_label|default('Ajouter le modèle de console') }}</button>
{{ form_end(form) }}
