{{ form_start(form) }}
    {#{{ form_widget(form) }}#}
    {{ form_errors(form) }}
    <div class="form-group">
        {{ form_label(form.nom, "Nom", {'label_attr': {'class': 'col-sm-2 control-label'}}) }}
        <div class="col-sm-10">
            {{ form_errors(form.nom) }}
            {{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
        </div>
    </div>

    {{ form_rest(form) }}

    <button class="btn">{{ button_label|default('Ajouter la marque') }}</button>
{{ form_end(form) }}
